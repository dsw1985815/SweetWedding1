.index_title {
  font-size: 18pt;
  /*字体大小*/
  font-weight: bold;
  /*字体加粗*/
  padding-bottom: 10rpx;
  /*底端内边距*/
  border-bottom: 2rpx dashed #c9a09a;
  /*边框*/
}
.pics_title {
  color: #c9a09a;
  font-size: 16px;
  background-color: #fff;
  border-top-left-radius: 5px;
  border-top-right-radius: 5px;
  padding: 10rpx;
}

.moreView{
  color: #c9a09a;
  font-size: 12px;
  border-top-right-radius: 5px;
  background-color: #fff;
  padding: 10rpx;
  position: absolute;
  right: 20rpx;
}

.main_btn {
  display: flex;
  /*flex模型布局*/
  flex-direction: row;
  /*水平布局*/
  align-items: top;
  /*垂直方向居中对齐*/
  font-size: 16pt;
  /*字体大小*/
  margin: 10rpx 0;
  /*外边距上下10rpx，左右0*/
  background-color: #c9a09a;

}

/* 会议地点区域：按钮样式 */
.main_btn2 {
  color: white;
  /*字体颜色*/
  background-color: #c9a09a;
  /*背景颜色*/
}

.main_box {
  border: 2rpx solid #c9a09a;
  /*边框*/
  color: #c9a09a;
  /*字体颜色*/
  margin: 15rpx;
  /*外边距*/
  padding: 15rpx;
  /*内边距*/
}

/* 区域标题样式 */
.main_title {
  font-size: 18pt;
  /*字体大小*/
  font-weight: bold;
  /*字体加粗*/
  padding-bottom: 10rpx;
  /*底端内边距*/
  border-bottom: 2rpx dashed #c9a09a;
  /*边框*/
}

.myLogin{
  background-color: #c9a09a;
  height: 400rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}

.index_button {
  font-size: 16px;
  margin-left: 10%;
  width: 80%;
  background-color: #c9a09a;
  color: #fff;
  position: fixed;
  bottom: 100rpx;
  text-align: center;
  height: 85rpx;
  line-height: 85rpx;
  border-radius: 10rpx;
}

.index_number {
  color: #d4237a;
  position: fixed;
  bottom: 10%;
  font-size: 80px;
  font-weight: bold;
  text-align: center;
  width: 100%;
}
